{
    "name": "local/phlexus-module-users",
    "license": "BSD-3-Clause",
    "description": "Phlexus Users Module",
    "repositories": [
        {
            "type": "path",
            "url": "../phlexus-core"
        },
        {
            "type": "path",
            "url": "../phlexus-library-auth"
        }
    ],
    "require": {
        "local/phlexus-core": "*",
        "local/phlexus-library-auth": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "phalcon/ide-stubs": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Phlexus\\Modules\\BaseUser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}